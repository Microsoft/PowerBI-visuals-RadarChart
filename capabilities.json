                            },
                            {
                                "value": "Bottom",
                                "displayName": "Bottom"
                            },
                          {
                            "value": "Left",
                            "displayName": "Left"
                          },
                          {
                                "value": "Right",
                                "displayName": "Right"
                            },
                          {
                                "value": "TopCenter",
                                "displayName": "Top Center"
                            },
                          {
                                "value": "BottomCenter",
                                "displayName": "Bottom Center"
                            },
                          {
                                "value": "LeftCenter",
                                "displayName": "Left Center"
                            },
                          {
                                "value": "RightCenter",
                                "displayName": "Right Center"
                            }
                        ]
                    },
                    "displayName": "Position"
                }
            }
        },
        "dataPoint": {
            "displayName": "Data colors",
            "description": "Display data color options",
            "properties": {
                "fill": {
                    "displayName": "Fill",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        },
        "line": {
            "displayName": "Draw Lines",
            "properties": {
                "show": {
                    "displayName": "Draw Lines",
                    "type": {
                        "bool": true
                    }
                },
                "lineWidth": {
                    "displayName": "Line Width",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "displaySettings": {
            "displayName": "Display settings",
            "properties": {
                "minValue": {
                    "displayName": "Axis start",
                    "type": {
                        "numeric": true
                    }
                },
                "axisBeginning": {
                    "displayName": "Axis start position",
                    "type": {
                        "enumeration": [
                            {
                                "value": "-1",
                                "displayName": "North"
                            },
                            {
                                "value": "1",
                                "displayName": "South"
                            }
                        ]
                    }
                }
            }
        },
