                    "type": {
                        "bool": true
                    }
                },
                "titleText": {
                    "displayName": "Name",
                    "description": "Title Text",
                    "type": {
                        "text": true
                    },
                    "suppressFormatPainterCopy": true
                },
                "labelColor": {
                    "displayName": "Color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
              "fontSize": {
                "displayName": "Text Size",
                "type": {
                  "formatting": {
                    "fontSize": true
                  }
                }
              },
              "position": {
                    "type": {
                        "enumeration": [
                            {
                                "value": "Top",
