{
    "dataRoles": [
        {
            "displayName": "Category",
            "name": "Category",
            "kind": "Grouping"
        },
        {
            "displayName": "Y Axis",
            "name": "Y",
            "kind": "Measure"
        }
    ],
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "Category": {
                        "min": 0,
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": {
                        "in": "Category"
                    },
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
                "values": {
                    "select": [
                        {
                            "bind": {
                                "to": "Y"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "objects": {
        "legend": {
            "displayName": "Legend",
            "description": "Display Legend Options",
            "properties": {
                "show": {
                    "displayName": "Show",
                    "type": {
                        "bool": true
                    }
                },
                "showTitle": {
                    "displayName": "Title",
